{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "globals": {
        "ko": false,
        "angular": true,
        "_": true
    },
    "rules": {
        "no-console":0,
        "indent": [
            "error",
            4
        ],
        "quotes": [
            "warn",
            "double"
        ],
        "semi": [
            "error",
            "always",
            { "omitLastInOneLineBlock": true }
        ],
        "space-before-function-paren": [
            "warn",
            {
                "anonymous": "always",
                "named": "always",
                "asyncArrow": "ignore"
            }
        ],
        "keyword-spacing": [
            "error",
            {
                "before": true,
                "after": true
            }
        ],
        "no-unused-vars": ["warn", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
    }
}